{
  "$schema": "http://json.schemastore.org/template",
  "author": "BepInEx, 10_days_till_xmas",
  "classifications": [ "BepInEx", "BepInEx 5", "Plugin", "Ultrakill" ],
  "identity": "UltraModding.BarePlugin",
  "name": "Ultrakill BepInEx Plugin",
  "shortName": "ultramodbareplugin",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "preferNameDirectory": true,
  "sourceName": "UltraModding.BarePluginTemplate",
  "symbols": {
    "TargetFramework": {
      "type": "parameter",
      "description": "The target framework for the project",
      "datatype": "text",
      "defaultValue": "net35",
      "replaces": "net35"
    },
    "Description": {
      "type": "parameter",
      "description": "Plugin description",
      "datatype": "text",
      "replaces": "My first plugin",
      "defaultValue": "My first plugin"
    },
    "UnityVersion": {
      "type": "parameter",
      "description": "Unity version to use when developing the plugin",
      "datatype": "text",
      "replaces": "5.6.0",
      "defaultValue": "5.6.0"
    },
    "Version": {
      "type": "parameter",
      "description": "Plugin version",
      "datatype": "text",
      "replaces": "{version}",
      "defaultValue": "1.0.0"
    }
  }
}